{
  "cli": {
    "analytics": false
  },
  "version": 1,
  "projects": {
    "core": {
      "projectType": "library",
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "prefix": "ngrx-ducks",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/core"
          ],
          "options": {
            "project": "libs/core/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/core/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/core/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/core"
          ],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/core/src/**/*.ts",
              "libs/core/src/**/*.html"
            ]
          }
        }
      },
      "tags": [
        "lib"
      ]
    },
    "ngrx-ducks": {
      "projectType": "application",
      "root": "apps/ngrx-ducks",
      "sourceRoot": "apps/ngrx-ducks/src",
      "prefix": "ngrx-ducks-mono",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/ngrx-ducks",
            "index": "apps/ngrx-ducks/src/index.html",
            "main": "apps/ngrx-ducks/src/main.ts",
            "polyfills": "apps/ngrx-ducks/src/polyfills.ts",
            "tsConfig": "apps/ngrx-ducks/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/ngrx-ducks/src/favicon.ico",
              "apps/ngrx-ducks/src/assets"
            ],
            "styles": [
              "apps/ngrx-ducks/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ngrx-ducks/src/environments/environment.ts",
                  "with": "apps/ngrx-ducks/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ngrx-ducks:build:production"
            },
            "development": {
              "browserTarget": "ngrx-ducks:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngrx-ducks:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ngrx-ducks/src/**/*.ts",
              "apps/ngrx-ducks/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/ngrx-ducks"
          ],
          "options": {
            "jestConfig": "apps/ngrx-ducks/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "ngrx-ducks-e2e": {
      "root": "apps/ngrx-ducks-e2e",
      "sourceRoot": "apps/ngrx-ducks-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ngrx-ducks-e2e/cypress.json",
            "devServerTarget": "ngrx-ducks:serve:development",
            "tsConfig": "apps/ngrx-ducks-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ngrx-ducks:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/ngrx-ducks-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "ngrx-ducks"
      ]
    },
    "schematics": {
      "projectType": "library",
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "prefix": "ngrx-ducks",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/schematics"
          ],
          "options": {
            "project": "libs/schematics/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/schematics/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/schematics/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/schematics"
          ],
          "options": {
            "jestConfig": "libs/schematics/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/schematics/src/**/*.ts",
              "libs/schematics/src/**/*.html"
            ]
          }
        }
      },
      "tags": [
        "lib"
      ]
    },
    "workspace": {
      "root": ".",
      "architect": {
        "version": {
          "builder": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true
          }
        }
      }
    }
  }
}
